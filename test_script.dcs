#!/usr/local/bin/DCS
printathing:
	p %ARG_0 has been printed

main:
	print Help and Stuff
	wait
	p You can use shorthand
	w

	if true
		p Conditionals
		? true
			p Embedded Conditionals!

	p Take program/function arguments through %ARG_0 %ARG_1 etc.
	w
	call printathing Function_Calls!

	%an_alias = call printathing
	%print_alias Aliasing of stuff!

	%VARIABLES = 0
	if %VARIABLES < 5 
		p Loop %VARIABLES
		w
		%VARIABLES = %VARIABLES + 1
		go -4
